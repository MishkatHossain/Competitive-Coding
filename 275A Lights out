#include<bits/stdc++.h>
using namespace std;

int main()
{
 int    x11, x12, x13, 
        
        x21, x22, x23, 
        
        x31, x32, x33;
        
    
    cin >> x11 >> x12 >> x13      //Taking input
        
        >> x21 >> x22 >> x23 
        
        >> x31 >> x32 >> x33;
    
    
cout << ((x11 + x12 + x21) % 2 == 0 ? "1" : "0")               // for (i,j) = (1,1) 
        << ((x11 + x12 + x13 + x22) % 2 == 0 ? "1" : "0")           // for (i,j) = (1,2)
            << ((x12 + x13 + x23) % 2 == 0 ? "1" : "0") << endl;        // for (i,j) = (1,3)

            
cout << ((x11 + x21 + x22 + x31) % 2 == 0 ? "1" : "0")          // for (i,j) = (2,1)
        << ((x12 + x21 + x22 + x23 + x32) % 2 == 0 ? "1" : "0")     // for (i,j) = (2,2)
            << ((x13 + x22 + x23 + x33) % 2 == 0 ? "1" : "0") << endl;  // for (i,j) = (2,3)
            
            
cout << ((x21 + x31 + x32) % 2 == 0 ? "1" : "0")                // for (i,j) = (3,1)
        << ((x22 + x31 + x32 + x33) % 2 == 0 ? "1" : "0")           // for (i,j) = (3,2)
            << ((x23 + x32 + x33) % 2 == 0 ? "1" : "0") << endl;        // for (i,j) = (3,3)
    
    
    return 0;

}
